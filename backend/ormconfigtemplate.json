{
  "type": "postgres",
  "host": "YOUR_HOST_POSTGRES",
  "port": 5432,
  "username": "YOUR_USERNAME_DATABASE",
  "password": "YOUR_PASSWORD_DATABASE",
  "database": "YOUR_DATABASE_NAME",
  "logging": false,
  "entities": [
     "src/app/models/*.ts"
  ],
  "migrations": [
     "src/database/migrations/*.ts"
  ],
  "cli": {
    "migrationsDir": "src/database/migrations"
  }
}
